{
  "name": "nocodb-mcp-server",
  "version": "1.0.0",
  "description": "Production NocoDB MCP Server for Cursor IDE integration",
  "main": "server.js",
  "scripts": {
    "start": "uvicorn mcp-server-fastapi:app --host 0.0.0.0 --port 3001",
    "dev": "uvicorn mcp-server-fastapi:app --host 0.0.0.0 --port 3001 --reload",
    "test": "curl http://localhost:3001/health",
    "docker-build": "docker build -t gordo-v1su4/nocodb-mcp-server .",
    "docker-push": "docker push gordo-v1su4/nocodb-mcp-server",
    "docker-run": "docker run -p 3001:3001 --env-file local.env gordo-v1su4/nocodb-mcp-server",
    "compose-up": "docker-compose up -d",
    "compose-down": "docker-compose down",
    "compose-logs": "docker-compose logs -f nocodb-mcp-server",
    "compose-test": "curl http://localhost:3001/health"
  },
  "keywords": [
    "nocodb",
    "mcp",
    "cursor",
    "discord",
    "automation",
    "api"
  ],
  "author": "gordo-v1su4",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/gordo-v1su4/mcp-server.git"
  },
  "homepage": "https://github.com/gordo-v1su4/mcp-server"
}
